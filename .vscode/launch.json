    "version": "0.2.0",
    "configurations": [  
               
        {
            "name": "docskin md",
            "type": "debugpy",
            "request": "launch",
            "module": "docskin",
            "args": ["md", "--input", "README.md", 
                "--output", "README.pdf"
            ]
        },
        {
            "name": "docskin github",
            "type": "debugpy",
            "request": "launch",
            "module": "docskin",
            "args": ["github", "--repo", "cdeimling/docskin", 
                "--issue", "1",
                "--output", "issue1.pdf"
            ]
        }
    ]
}