    "version": "0.2.0",
    "configurations": [
        {
            "name": "docskin github",
            "type": "debugpy",
            "request": "launch",
            "module": "docskin",
            "args": ["github", "--repo", "cdeimling/docskin",
                "--issue", "1",
                "--output", "issue1.pdf"
            ]
        },
        {
            "name": "docskin md",
            "type": "debugpy",
            "request": "launch",
            "module": "docskin",
            "args": [
                "-cs", "assets/corporate-sample-design.css",
                "-cc", "markdown-body mode-slide",
                "-l", "assets/github.svg.png",
                "md",
                "--input", "README.md",
                "--output", "README.pdf"
            ]
        },
        {
            "name": "docskin md-dir",
            "type": "debugpy",
            "request": "launch",
            "module": "docskin",
            "args": ["md-dir", "--input", "tests/resources/",
                "--output", "tests/output"
            ]
        },
        {
            "name": "docskin setup",
            "type": "debugpy",
            "request": "launch",
            "module": "docskin",
            "args": ["setup"
            ]
        }
    ]
}